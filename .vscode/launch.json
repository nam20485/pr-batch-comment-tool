{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: GitHubPrTool.Desktop Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/src/GitHubPrTool.Desktop/bin/Debug/net8.0/GitHubPrTool.Desktop.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/GitHubPrTool.Desktop",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": false,
            "requireExactSource": false,
            "suppressJITOptimizations": true,
            "enableStepFiltering": false,
            "symbolOptions": {
                "searchPaths": [],
                "searchMicrosoftSymbolServer": true,
                "searchNuGetOrgSymbolServer": true,                
                "moduleFilter": {
                    "mode": "loadAllButExcluded",
                    "excludedModules": []
                }
            },
            "sourceFileMap": {
                "/src": "${env:USERPROFILE}/.nuget/packages"
            }
        },
        {
            "name": "C#: GitHubPrTool.Desktop Debug (Attach)",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false,
            "requireExactSource": false,
            "suppressJITOptimizations": true,
            "enableStepFiltering": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true,
                "searchNuGetOrgSymbolServer": true,
                "moduleFilter": {
                    "mode": "loadAllButExcluded",
                    "excludedModules": []
                }
            }
        }
    ]
}